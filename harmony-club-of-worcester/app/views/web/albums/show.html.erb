<h1 class="text-center"><%= @photo.title %></h1>

<div>
  <%if @photo.image.attached?%>
    <%= image_tag @photo.image, alt: @photo.description, class:"mx-auto d-block w-75 my-4 img-thumbnail" %>
  <% end %>
</div>
<h3 class="text-center">Description:</h3>
<p class="text-center"><%= @photo.description %></p>

<div class="text-center">
  <%= link_to 'Back', web_albums_index_path, class: "btn btn-primary" %>
</div>